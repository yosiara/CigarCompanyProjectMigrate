<?xml version="1.0" encoding="utf-8"?>

<odoo>
    <data>
        <record id="paper_format_inventory" model="report.paperformat">
            <field name="name">Inventory Paper Format</field>
            <field name="default" eval="True"/>
            <field name="format">custom</field>
            <field name="page_height">297</field>
            <field name="page_width">210</field>
            <field name="orientation">Landscape</field>
            <field name="margin_top">30</field>
            <field name="margin_bottom">23</field>
            <field name="margin_left">5</field>
            <field name="margin_right">5</field>
            <field name="header_line" eval="False"/>
            <field name="header_spacing">20</field>
            <field name="dpi">90</field>
        </record>

        <record id="paperformat_vertical" model="report.paperformat">
            <field name="name">Paperformat vertical</field>
            <field name="default" eval="True"/>
            <field name="format">Letter</field>
            <field name="page_height">0</field>
            <field name="page_width">0</field>
            <field name="orientation">Portrait</field>
            <field name="margin_top">18</field>
            <field name="margin_bottom">14</field>
            <field name="margin_left">5</field>
            <field name="margin_right">5</field>
            <field name="header_line" eval="False"/>
            <field name="header_spacing">14</field>
            <field name="dpi">96</field>
        </record>

        <record id="paperformat_horizontal" model="report.paperformat">
            <field name="name">Paperformat vertical</field>
            <field name="default" eval="True"/>
            <field name="format">custom</field>
            <field name="page_height">0</field>
            <field name="page_width">0</field>
            <field name="orientation">Landscape</field>
            <field name="margin_top">18</field>
            <field name="margin_bottom">14</field>
            <field name="margin_left">15</field>
            <field name="margin_right">15</field>
            <field name="header_line" eval="False"/>
            <field name="header_spacing">14</field>
            <field name="dpi">96</field>
        </record>

        <record id="paperformat_equipment_label" model="report.paperformat">
            <field name="name">Paperformat Equipment Label</field>
            <field name="default" eval="True"/>
            <field name="format">custom</field>
            <field name="page_height">12</field>
            <field name="page_width">50</field>
            <field name="orientation">Portrait</field>
            <field name="margin_top">0</field>
            <field name="margin_bottom">0</field>
            <field name="margin_left">0</field>
            <field name="margin_right">0</field>
            <field name="header_line" eval="False"/>
            <field name="header_spacing">0</field>
            <field name="dpi">96</field>
        </record>

    </data>
</odoo>
